<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Student.StudentController">
   <children>
      <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Student Data">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <HBox layoutX="-1.0" layoutY="-3.0" prefHeight="375.0" prefWidth="600.0">
                           <children>
                              <TableView fx:id="studenttable" prefHeight="375.0" prefWidth="559.0">
                                <columns>
                                  <TableColumn fx:id="idcolumn" prefWidth="75.0" text="id" />
                                  <TableColumn fx:id="firstnamecolumn" prefWidth="75.0" text="Firstname" />
                                    <TableColumn fx:id="lastnamecolumn" prefWidth="75.0" text="Lastname" />
                                    <TableColumn fx:id="emailcolumn" prefWidth="75.0" text="email" />
                                    <TableColumn fx:id="dobcolumn" prefWidth="75.0" text="DOB" />
                                    <TableColumn fx:id="FILIEREcolumn" prefWidth="75.0" text="Filiere" />
                                    <TableColumn fx:id="MODULEcolumn" prefWidth="75.0" text="Module" />
                                </columns>
                              </TableView>
                              <Button mnemonicParsing="false" onAction="#loadStudentData" prefHeight="25.0" prefWidth="107.0" text="Load" />
                           </children>
                        </HBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
